$(' #article_<%= @entry.article.id %>').hide( 'blind', '', 200 );
<% if @entry.watch_id %>
  var watch_count = $('#watch-count').text();
  watch_count =  parseInt(watch_count);
  $("#watch-count").text(watch_count - 1 );
<% else %>
  var cart_count = $("#cart-count").text();
  cart_count =  parseInt(cart_count);
  $("#cart-count").text(cart_count - 1 );
<% end %>